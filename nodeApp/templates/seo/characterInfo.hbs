<!DOCTYPE html>
<html lang="en" prefix="og: http://ogp.me/ns#">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">

  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="google-site-verification" content="naBGHqk0ma5wZPtKpvobPwocUpZLrOPGybu7qjyAwkk" />
  <meta name="robots" content="index,follow">

  <meta name="description" content="{{description}}">
  <meta name="keywords" content="{{keywords}}">
  <meta name="author" content="Vicente Manuel Aleixos Bellido">

  <meta itemprop="name" content="{{title}}">
  <meta itemprop="description" content="{{description}}">
  <meta itemprop="image" content="https://soyto.org/assets/img/logo.png">

  <meta property="og:type" content="website">
  <meta property="og:url" content="https://soyto.org/character/{{character.serverName}}/{{character.characterID}}">
  <meta property="og:title" content="{{title}}">
  <meta property="og:description" content="{{description}}">
  <meta property="og:site_name" content="soyto.org">
  <meta property="og:image" content="https:{{picture}}">
  <meta property="og:image:type" content="image/jpg">
  <meta property="og:image:alt" content="{{title}}">

  <meta property="twitter:image" content="https:{{picture}}">

  <script type="application/ld+json" id="json-ld"></script>

  <!--[if IE]><link rel="shortcut icon" href="/favicon.ico"><![endif]-->
  <link rel="icon" href="/favicon.ico">

  <!-- Canonical URL -->
  <link rel="canonical" href="https://soyto.org/character/{{character.serverName}}/{{character.characterID}}">

  <!-- Title -->
  <title>{{title}}</title>

  <!-- bootstrap -->
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css" type='text/css' media='all' />
  <!-- font-awesome -->
  <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" type='text/css' media='all' />
  <!-- Angular loading bar -->
  <link rel="stylesheet" href='//cdnjs.cloudflare.com/ajax/libs/angular-loading-bar/0.7.1/loading-bar.min.css' type='text/css' media='all' />
  <!-- Angular-motion -->
  <link rel="stylesheet" href="/assets/lib/bower/angular-motion/dist/angular-motion.min.css">
  <!-- Site -->
  <link rel="stylesheet" href="/assets/dist/site.css" type="text/css" media="all" />

  <!-- Facebook -->
  <meta property="fb:app_id" content="883135418439345" />

  <!-- Bing -->
  <meta name="msvalidate.01" content="7A31A668D813B8C91E82BBCFD25AFEE4" />

</head>
<body>

  <nav class="navbar navbar-fixed-top" id="header">
 	  <div class="col-md-12">

      <div class="navbar-header">
        <a href="/" class="navbar-brand">Soyto ranking tool</a>
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse">
          <i class="fa fa-bars"></i>
        </button>
      </div><!-- /.navbar-header -->

      <div class="collapse navbar-collapse" id="navbar-collapse">
        <ul class="nav navbar-nav navbar-right">
          <li><a href="/">Home</a></li>
          <li ><a href="/twitchChannels">AION Twitch</a></li>
          <li>
            <a class="dropdown-toggle" data-toggle="dropdown">External links</a>
            <ul class="dropdown-menu">
              <li>
                <a href="//github.com/soyto/ranking-soyto" target="_blank">
                  <i class="fa fa-github-alt"></i> Project
                </a>
              </li>
              <li>
                <a href="https://discord.gg/hy9SzwB" target="_blank">
                  <i class="fa fa-wechat"></i> AION EU Discord
                </a>
              </li>
              <li>
                <a href="//github.com/soyto/soyto.github.io/issues" target="_blank">
                  <i class="fa fa-warning"></i> Issues
                </a>
              </li>
              <li>
                <a href="//board.en.aion.gameforge.com/board6-aion-the-community/board56-aion-fanbase/board135-user-projects/23519-soyto-ranking-tool/" target="_blank">
                <i class="fa fa-paperclip"></i> Forum
                </a>
              </li>
              <li>
                <a href="//www.facebook.com/soyto.github.io/" target="_blank">
                  <i class="fa fa-facebook"></i> Facebook
                </a>
              </li>
            </ul>
          </li>
        </ul>
      </div><!-- /.collapse navbar-collapse -->

    </div><!-- /.col-md-12 -->
  </nav>

  <div class="container" id="main">

    <ol class="breadcrumb">
      <li><a href="/">Home</a></li>
      <li><a href="/ranking/{{character.serverName}}">{{character.serverName}}</a></li>
      <li><a href="/ranking/{{character.serverName}}/{{character.characterID}}">{{character.characterName}}</a></li>
    </ol>

    <div class="row">

      {{!-- Left column --}}
      <div class="col-md-4 col-xs-12">

        {{!-- Character picture and name --}}
        <div class="panel panel-default" itemscope itemtype="http://schema.org/Person">

          <!-- Panel thumbnail -->
          <div class="panel-thumbnail">

            <!-- Image -->
            <img src="{{picture}}" class="img img-responsive" itemprop="image"><!-- /Image -->

            <!-- Social icons -->
            <div class="social-icons" data-ng-if="character.social">

              {{#if social.buttons.facebook}}
              <!-- Facebook -->
              <a href="{{social.buttons.facebook}}" target="_blank">
                <i class="fa fa-facebook-square fa-2x"></i>
              </a><!-- /Facebook -->
              {{/if}}

              {{#if social.buttons.youtube}}
              <!-- Youtube -->
              <a href="{{social.buttons.youtube}}" target="_blank">
                <i class="fa fa-youtube-square fa-2x"></i>
              </a><!-- /Youtube -->
              {{/if}}

              {{#if social.buttons.twitch}}
              <!-- Twitch -->
              <a href="{{social.buttons.twitch}}" target="_blank">
                <i class="fa fa-twitch fa-2x"></i>
              </a><!-- /Twitch -->
              {{/if}}

              {{#if social.buttons.mouseClick_gearCalcID}}
              <!-- Mouseclick -->
              <a href="{{social.buttons.mouseClick_gearCalcID}}" target="_blank">
                <i class="fa fa-address-card fa-2x"></i>
              </a><!-- /Mouseclick -->
              {{/if}}

            </div><!-- /Social icons -->

          </div><!-- /Panel thumbnail -->

          <!-- Panel body -->
          <div class="panel-body">
            <h4><span itemprop="name">{{character.characterName}}</span> <small>{{getCharacterRaceName character.raceID}} {{getCharacterRankName character.soldierRankID}}</small></h4>
            <h5><span itemprop="jobTitle">{{getCharacterClassName character.characterClassID}}</span>
              {{#if (not (null character.guildID))}}
              <span itemprop="memberOf" itemscope itemtype="http://schema.org/Organization">of <span itemprop="name">{{character.guildName}}</span></span>
              {{else}}
              <span>without guild</span>
              {{/if}}
            </h5>
          </div><!-- /Panel body -->

        </div>
        {{!-- /Character picture and name --}}

        {{!-- Search panel --}}
        <div class="panel panel-search">

          <!-- Header -->
          <div class="panel-heading"><h4>Search another character:</h4></div><!-- /header -->

          <!-- Body -->
          <div class="panel-body panel-search-body">

          <!-- Search form -->
          <div class="form-group">
            <div class="input-group">
              <input type="text" class="form-control">
              <span class="input-group-addon"><i class="fa fa-search"></i></span>
            </div>
          </div><!-- /Search form -->

          </div><!-- /Body -->

        </div>
        {{!-- /Search panel --}}

        {{!-- Names Panel --}}
        <div class="panel">
          <div class="panel-heading"><h4>Names</h4></div>
          <div class="panel-body">
            <table class="table table-striped table-hover">
              <thead>
              <tr>
                <th>Date</th>
                <td>Name</td>
              </tr>
              </thead>
              <tbody>
              {{#each character.names}}
              <tr>
                <td>{{ formatDate this.date}}</td>
                <td>{{this.characterName}}</td>
              </tr>
              {{/each}}
              </tbody>
            </table>
          </div>
        </div>
        {{!-- /Names Panel --}}

        {{!-- Legions Panel --}}
        <div class="panel">
          <div class="panel-heading"><h4>Legions</h4></div>
          <div class="panel-body">
            <table class="table table-striped table-hover">
              <thead>
              <tr>
                <th>Date</th>
                <td>Guild name</td>
              </tr>
              </thead>
              <tbody>
              {{#each character.guilds }}
              <tr>
                <td>{{ formatDate this.date }}</td>
                <td>{{ this.guildName }}</td>
              </tr>
              </tbody>
              {{/each}}
            </table>
          </div>
        </div>
        {{!-- /Legions Panel --}}

      </div>
      {{!-- /Left column --}}

      {{!-- Right column --}}
      <div class="col-md-8 col-xs-12">

        {{!-- Rank History --}}
        <div class="panel">
          <div class="panel-heading">
            <ul class="nav nav-tabs">
              <li role="presentation active"><a href="">Table</a></li>
            </ul>
          </div>
          <div class="panel-body">
            <table class="table table-striped table-hover">
              <thead>
            <tr>
              <th>Date</th>
              <th>Rank</th>
              <th>Position</th>
              <th>Position change</th>
              <th>Glory points</th>
              <th>Glory points change</th>
            </tr>
            </thead>
            <tbody>
              {{#each character.status}}
              <tr>
                <td><a href="/ranking/{{../character.serverName}}/{{this.date}}">{{formatDate this.date}}</a></td>
                <td>{{getCharacterRankName this.soldierRankID}}</td>
                <td>{{this.position}}</td>
                <td>
                  <i class="fa {{#if (gt this.rankingPositionChange 0)}}fa-arrow-up text-success{{/if}}{{#if (lt this.rankingPositionChange 0)}}fa-arrow-down text-danger{{/if}}"></i>
                  {{this.rankingPositionChange}}
                </td>
                <td>{{this.gloryPoint}}</td>
                <td>
                  <i class="fa {{#if (gt this.gloryPointChange 0)}}fa-arrow-up text-success{{/if}}{{#if (lt this.gloryPointChange 0)}}fa-arrow-down text-danger{{/if}}"></i>
                  {{this.gloryPointChange}}
                </td>        
              </tr>      
              {{/each}}
            </tbody>
            </table>
          </div>
        </div>
        {{!-- /Rank History --}}

        {{!-- Facebook plugin --}}
        <div class="panel">
          <div class="panel-body">
            <div class="fb-comments" 
              data-href="http://soyto.github.io/#/character/{{character.serverName}}/{{character.characterID}}"
              data-numposts="50"
              data-colorsheme="light"></div>
          </div>
        </div>
        {{!-- /Facebook plugin --}}

      </div>
      {{!-- /Right column --}}

    </div>

    <footer class="footer">
      <hr>
      <div class="col-md-12 text-center">
        <form action="//www.paypal.com/cgi-bin/webscr" method="post" target="_top" class="ng-pristine ng-valid">
          <input type="hidden" name="cmd" value="_s-xclick">
          <input type="hidden" name="encrypted" value="-----BEGIN PKCS7----------END PKCS7-----">
          <input type="image" src="//www.paypalobjects.com/en_US/i/btn/btn_donate_SM.gif" name="submit" alt="PayPal - The safer, easier way to pay online!">
          <img alt="" src="//www.paypalobjects.com/es_ES/i/scr/pixel.gif" width="1" height="1">
        </form>
        <p>© <a href="mailto:ranking.soyto@hotmail.com">Vicente Manuel Aleixos Bellido</a> (But nobody was working on this on 1999) </p>
      </div>
      <hr>
    </footer>
  </div>

  
